#while문 사용하여 구구단 출력
num = int(input())
count = 1

while count <= 9 :
    print(f"{num} x {count} = {num*count}")
    count +=1

#while문 사용하여 구구단 출력 > 입력을 문자로 받을 경우
num = input()
tlqkf = ord(num)
count = 1

while count <= 9 :
    print(f"{tlqkf} x {count} = {tlqkf*count}")
    count +=1

#for문 사용하여 구구단 출력
num = int(input())

for i in range(1,10):
    print(f"{num} x {i} = {num*i}")
