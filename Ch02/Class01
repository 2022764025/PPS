class Rectangle:
    def __init__(self, width, height):
        self.width = width
        self.height = height
       # self.area = width * height

    #area뒤에 () 붙여야 함
    def area(self):
        return self.width * self.height

width = int(input())
height = int(input())

rect = Rectangle(width, height)

print(f"가로 입력:{rect.width}")
print(f"세로 입력:{rect.height}")
#print(f"넓이:{rect.area()}")
print(f"넓이:{rect.area()}")
